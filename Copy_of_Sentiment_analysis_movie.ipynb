      "source": [
        "def hybrid_predict(texts, threshold_low=0.4, threshold_high=0.6):\n",
        "    if isinstance(texts, str):\n",
        "        texts = [texts]\n",
        "\n",
        "    X_input = cv.transform(texts).toarray()\n",
        "\n",
        "    # Get probabilities for positive class\n",
        "    proba1 = model2.predict_proba(X_input)[:, 1]\n",
        "    proba2 = model.predict_proba(X_input)[:, 1]\n",
        "\n",
        "    # You could also try weighted average if one model is better\n",
        "    # avg_proba = 0.6 * proba1 + 0.4 * proba2\n",
        "    avg_proba = (proba1 + proba2) / 2\n",
        "\n",
        "    final_preds = []\n",
        "    for prob in avg_proba:\n",
        "        if prob > threshold_high:\n",
        "            final_preds.append(\"positive\")\n",
        "        elif prob < threshold_low:\n",
        "            final_preds.append(\"negative\")\n",
        "        else:\n",
        "            final_preds.append(\"neutral\")\n",
        "\n",
        "    return final_preds"
      ],
      "metadata": {
        "id": "lkxybi9SPBz6"
      },
      "execution_count": null,
      "outputs": []
